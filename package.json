{
  "name": "vatrochat",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run build && cd ./frontend && npm install && cd ../backend && pip install -r requirements.txt",
    "start": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd ../backend && gunicorn -w 4 -k uvicorn.workers.UvicornWorker main:app",
    "frontend": "cd ../frontend && npm run serve"
  },
  "dependencies": {
    "concurrently": "^6.2.1"
  },
  "main": "index.js",
